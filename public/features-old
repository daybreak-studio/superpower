                <style>
.spectrum-main-graph-image.card-hover.active .y-axis-label {
	opacity: 0!important;
}
.y-axis-label {
	opacity: 0;
  transform: translateY(0px);
  transition: 0.2s ease-out opacity;
}
.graph-line {
	transition: 0.8s ease;
}
.quadrant {
	transition: 0.8s ease;
}
.quadrant.active {
	opacity: 1!important;
}
.quadrant > rect {
  transition: 0.8s ease;
}
.quadrant.active > rect {
	opacity: 0.05;
}
.point circle {
  opacity: 0;
  transition: 0.6s ease-out;
}
.point rect {
  transition: 0.4s ease-out;
}
.quadrant.active .point circle {
  opacity: 0.2;
}
.quadrant.active .point rect:nth-child(2) {
	fill: #11C182;
}
.quadrant.active .point rect:nth-child(3) {
	stroke: #000;
  opacity: 0.15;
}
.date-label {
  transition: 0.2s ease;
}
.quadrant.active .date-label {
  opacity: 1;
}
</style>
                <script>
const cardHover = document.querySelector('.spectrum-main-graph-image.card-hover')
const yAxisLabel = document.querySelector('.y-axis-label')
const yAxisText = document.querySelector('.y-axis-label text')
const graphLine = document.querySelector('.graph-line')
const quadrants = document.querySelectorAll('.quadrant')
cardHover.addEventListener("mousemove", (event) => {
  var rect = cardHover.getBoundingClientRect();
  var height = rect.height;
  var y = event.clientY - rect.top;
  var relativeValue = (y / height) * 100
  const minPercentage = 20.5
  const maxPercentage = 88.5
  let graphPercentage = 0
  if ((relativeValue > minPercentage) && (relativeValue < maxPercentage)) {
  	yAxisLabel.style.opacity = '1'
  	graphPercentage = (relativeValue - minPercentage) * (1 / (maxPercentage - minPercentage))
    var graphValue = (1645 * (1 - graphPercentage)).toString()
    var integer = graphValue.split(".")[0]
    var decimal = graphValue.split(".")[1].substring(0, 1)
    yAxisText.innerHTML = `${integer}<tspan>.${decimal}</tspan>`
    //console.log(graphValue)
  } else {
  	yAxisLabel.style.opacity = '0'
  }
  yAxisLabel.style.transform = `translateY(${relativeValue}%)`
})
cardHover.addEventListener("mouseout", (event) => {
  yAxisLabel.style.opacity = '0'
})
quadrants.forEach((item, i) => {
	item.querySelector('.hover-target').addEventListener("mouseover", (event) => {
    for (let j = 0; j < quadrants.length; j++) {
    	quadrants[j].style.opacity = '0.2'
    }
    graphLine.style.opacity = '0.2'
    cardHover.classList.add('active')
    item.classList.add('active')
    item.querySelector('.point circle').setAttribute('r', 24);
  })
  item.querySelector('.hover-target').addEventListener("mouseout", (event) => {
    for (let j = 0; j < quadrants.length; j++) {
    	quadrants[j].style.opacity = '1'
    }
    graphLine.style.opacity = '1'
    cardHover.classList.remove('active')
    item.classList.remove('active')
    item.querySelector('.point circle').setAttribute('r', 10);
  })
})
</script>
              </div>
            </div>
