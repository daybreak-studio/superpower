                <script>
	const biomarkerImages = document.querySelectorAll('.spectrum-main-graph-image._1 img')
  biomarkerImages.forEach((item, i) => {
    item.addEventListener("mouseover", (event) => {
    	for (let j = 0; j < biomarkerImages.length; j++) {
        biomarkerImages[j].style.opacity = '0.6'
      }
      item.style.opacity = '1'
    });
    item.addEventListener("mouseout", (event) => {
    	for (let j = 0; j < biomarkerImages.length; j++) {
        biomarkerImages[j].style.opacity = '1'
      }
    });
  })
</script>
