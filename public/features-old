                <script>
	const card2 = document.querySelector('.spectrum-card-item._2')
	const products = document.querySelectorAll('.im-tired')
  card2.addEventListener("mousemove", (event) => {
  	const rect = card2.getBoundingClientRect();
    const x = event.clientX - rect.left;
    const y = event.clientY - rect.top;
    var moveFast = `translate(${(x / 16)}px,${(y / 16)}px)`
    var moveSlow = `translate(${(x / 32)}px,${(y / 32)}px)`
    products[0].style.transform = moveSlow
    products[1].style.transform = moveFast
    products[2].style.transform = moveSlow
  })
  card2.addEventListener("mouseout", (event) => {
  })
</script>
